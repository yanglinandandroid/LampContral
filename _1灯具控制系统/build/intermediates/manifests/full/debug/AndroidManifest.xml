<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app3"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/deng"
        android:label="@string/app_name" >
        <activity android:name="com.example.app3.MainActivity" >
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"
                    android:theme="@style/ContentOverlay"
                    android:windowSoftInputMode="adjustResize" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.app3.TreeActivity" />
        <activity
            android:name="com.example.app3.ControllerActivity"
            android:screenOrientation="landscape"
            android:theme="@style/ContentOverlay"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.example.app3.LightActivity"
            android:theme="@style/ContentOverlay"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.example.app3.GroupActivity"
            android:theme="@style/ContentOverlay"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.example.app3.SceneActivity"
            android:theme="@style/ContentOverlay"
            android:windowSoftInputMode="adjustResize" />
    </application>

</manifest>